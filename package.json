{
  "name": "sihl-example-issues",
  "version": "0.0.1",
  "scripts": {
    "build": "bsb -make-world && parcel build static/index.html --public-url /asset/",
    "watch:build": "bsb -make-world -w",
    "start": "yarn build && yarn sihl start",
    "clean": "bsb -clean-world",
    "bundle": "parcel build static/index.html --public-url /asset/",
    "watch:bundle": "parcel watch static/index.html --public-url /asset/",
    "sihl": "node src/Main.bs.js sihl",
    "test": "DB_USER=root DB_HOST=localhost DB_PASSWORD=password DB_NAME=dev DB_PORT=3306 jest"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "Josef Erben josef@oxidizing.io",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "bs-fetch": "^0.5.1",
    "bs-let": "^0.1.16",
    "bulma": "^0.8.0",
    "compression": "^1.7.4",
    "decco": "^1.1.1",
    "parcel-bundler": "^1.12.4",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "reason-react": "^0.7.0",
    "@sihl/core": "0.0.6",
    "@sihl/users": "0.0.6"
  },
  "devDependencies": {
    "bs-platform": "^7.1.0"
  }
}
